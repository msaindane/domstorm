<% include ../layouts/header.ejs %>
<% include ../layouts/navbar.ejs %>


<div class="container">  
	<h1><%= title %></h1>


<div class="well panel-heading">
  <form class="bs-example form-horizontal" action="/modules/create" method="POST">
	<fieldset>
	  <legend>Create a new 'Enum Function' module</legend>
	  <div class="form-group">
		<label for="inputName" class="col-lg-2 control-label">Name</label>
		<div class="col-lg-10">
		  <input type="text" name="_name" class="form-control" id="inputName" placeholder="Properties of document.window">
		</div>
	  </div>
	  <div class="form-group">
		<label for="inputDesc" class="col-lg-2 control-label">Description</label>
		<div class="col-lg-10">
		  <input type="text" class="form-control" name="_desc" id="inputDesc" placeholder="Lists all the document.window properties">
		</div>
	  </div>

	  <div class="form-group">
		<label class="col-lg-2 control-label">Module Type</label>
		<div class="col-lg-10">
		  <div class="radio">
			<label>
			  <input type="radio" name="_module_type" id="optionsRadios1" value="ENUM_FUNCTION" checked="">
			  Enumeration Functions
			</label>
		  </div>
		  <div class="radio">
			<label>
			  <input type="radio" name="_module_type" id="optionsRadios2" value="FUZZER">
			  Fuzzer
			</label>
		  </div>
		</div>
	  </div>

	  <div class="form-group">
		<label class="col-lg-2 control-label">Results Type</label>
		<div class="col-lg-10">
		  <div class="radio">
			<label>
			  <input type="radio" name="_results_type" id="optionsRadios1" value="SIMPLE_TABLE" checked="">
			  Simple Table Format
			</label>
		  </div>
		  <div class="radio">
			<label>
			  <input type="radio" name="_results_type" id="optionsRadios2" value="ENUM_LIST">
			  Enumeration List
			</label>
		  </div>
		</div>
	  </div>	  
	  
	  <div class="form-group">
		<label for="userScript" class="col-lg-2 control-label">User Script</label>
		<div class="col-lg-10">
		  <textarea name="_userScript" class="form-control" rows="7" id="userScript"></textarea>
		  <span class="help-block">Come up with your own script to do a Enumeration Function scan. Read more <u>here</u></span>
		</div>
	  </div>	  

	  <div class="form-group">
		<label for="userScript" class="col-lg-2 control-label">Enumeration Data</label>
		<div class="col-lg-10">
		  <textarea name="_enum_data" class="form-control" rows="3" id="enum_data"></textarea>
		  <span class="help-block">Specify your Enumeration Data here as a code. 
			  <code>
			  var data = [['window', 'postMessage.name'], ['document', 'domain']];
			  </code>
		  </span>
		</div>
	  </div>	
	  
	  <div class="form-group">
		<div class="col-lg-10 col-lg-offset-2">
		  <button class="btn btn-default" type="reset">Clear</button> 
		  <button type="submit" class="btn btn-primary">Submit</button> 
		</div>
	  </div>
	</fieldset>
  </form>
</div>			
</div>
<% include ../layouts/footer.ejs %>			